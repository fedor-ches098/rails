<div class="card">
  <div class="card-header">
    <h1><%= t(".header") %></h1>
  </div>
  <div class="card-body">
    <% if @tests.present? %>
      <table>
        <tr>
          <th><%= t(".title") %></th>
          <th><%= t(".level") %></th>
          <th><%= t(".questions") %></th>
          <th><%= t(".actions") %></th>
        </tr>

        <%= render @tests %>

      </table>
    <% else %>
      <p><%= t('.no_tests_available') %></p>
    <% end %>
  </div>
</div>
