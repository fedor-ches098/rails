<h1><%= t(".header") %></h1>

<% if @tests.present? %>
  <table>
    <tr>
      <th><%= t(".title") %></th>
      <th><%= t(".level") %></th>
      <th><%= t(".questions") %></th>
      <th><%= t(".actions") %></th>
    </tr>

    <%= render @tests %>

  </table>
<% else %>
  <p><%= t('.no_tests_available') %></p>
<% end %>
