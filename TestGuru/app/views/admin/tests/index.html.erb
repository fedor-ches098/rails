<div class="card">
  <div class="card-header">
    <h1><%= t(".header") %></h1>
  </div>
  <div class="card-body">
    <p>
      <%= link_to t(".create"), new_admin_test_path %>
    </p>

    <table>
      <tr>
        <th><%= t(".title") %></th>
        <th><%= t(".level") %></th>
        <th><%= t(".questions") %></th>
        <th colspan="4"><%= t(".actions") %></th>
      </tr>

      <%= render @tests %>
    </table>
  </div>
</div>
