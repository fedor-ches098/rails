<%= form_with model: [:admin, @test], local: true do |form| %>

  <%= render 'shared/errors', resource: @test %>
  
  <p>
    <%= form.label :title %>
    <%= form.text_field :title, id: :test_title, placeholder: 'Title' %>
  </p>

  <%= form.label :level %>
  <%= form.text_field :level, id: :test_level %>

  <p>
    <%= form.label :public %>
    <%= form.check_box :public, id: :test_public %>
  </p>
   
  <p>
    <%= form.label :category_id %>
    <%= form.collection_select :category_id, Category.all, :id, :title, prompt: true %>
  </p>



  <p><%= form.submit %></p>
<% end %>
